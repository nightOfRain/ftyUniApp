<view><cu-custom vue-id="48e98fda-1" bgColor="bg-blue" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">资料填写</view></cu-custom><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="cu-form-group"><view class="title">我的姓名</view><input placeholder="输入申请人姓名" name="name" value="{{formdata.name}}"/></view><view class="cu-form-group"><view class="title">审贷意图</view><picker value="{{formdata.housePurpose}}" range="{{picker}}" data-event-opts="{{[['change',[['PickerChange_zh',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(formdata.housePurpose>-1?picker[formdata.housePurpose]:'请选择审贷意图')+''}}</view></picker></view><view class="cu-form-group"><view class="title">期望额度</view><input placeholder="请输入期望额度" type="digit" name="LoanAmt" value="{{formdata.LoanAmt}}"/><text class="text-black">万</text></view><view class="cu-form-group"><view class="title">贷款期限</view><input placeholder="请输入期望期限" type="number" name="dkSqqx" value="{{formdata.dkSqqx}}"/><text class="text-black">月</text></view><view class="cu-form-group"><view class="title">手机号码</view><input placeholder="本人手机号" type="number" name="telno" value="{{formdata.telno}}"/><view data-event-opts="{{[['tap',[['getMsgCode',['$event']]]]]}}" class="cu-capsule radius" bindtap="__e"><view class="{{['cu-tag','bg-blue',isActive?'bg-grey':'']}}">{{''+(isActive?countStr:'获取验证码')+''}}</view></view></view><view class="cu-form-group"><view class="title">验证码</view><input placeholder="输入验证码" name="code" value="{{formdata.code}}"/></view><view class="cu-form-group margin-top"><textarea maxlength="-1" name="content" placeholder="需求详细描述" data-event-opts="{{[['input',[['textareaAInput',['$event']]]]]}}" value="{{content}}" bindinput="__e"></textarea></view><view class="padding-xl"><button class="cu-btn block bg-blue margin-tb-sm lg" form-type="submit">提交</button></view></form></view>